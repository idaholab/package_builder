module load ccache
module load moose-tools

EOF

    cat <<EOF > $PACKAGES_DIR/Modules/<MODULES>/civet/mpich-clang-petsc_alt-trilinos-opt
#%Module1.0#####################################################################
##
## Civet support module
## <PETSC_ALT>
