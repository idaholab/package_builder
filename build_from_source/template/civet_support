module load moose/.mpich_<TRILINOS>-clang-opt
module load ccache
module load moose-tools

EOF

    cat <<EOF > $PACKAGES_DIR/Modules/<MODULES>/civet/mpich-clang-petsc_alt-vtk-trilinos-dbg
